source 'https://rubygems.org'

gem 'rails', '3.2.7'
gem 'thin'
gem 'pg'
gem 'rails_12factor'

# Gems used only for assets and not required
# in production environments by default.
group :assets do
  gem 'sass-rails',   '~> 3.2.3'
  gem 'coffee-rails', '~> 3.2.1'
  gem 'uglifier', '>= 1.0.3'
end

gem 'jquery-rails'
gem 'eco'
gem 'httpclient'
gem 'twitter'

gem 'pry'
gem 'pry-rails'

group :development do
  gem 'heroku'
end

group :production do
	gem 'memcachier'
	gem 'dalli'
end

